{
  "name": "sensor-dashboard",
  "cors": {
    "allowed_origins": [
      "https://alefadham-droid.github.io",
      "http://localhost:8000",
      "http://localhost:5500",
      "*"
    ],
    "allowed_methods": ["GET", "OPTIONS"],
    "allowed_headers": ["Content-Type", "Authorization"],
    "max_age": 3600,
    "credentials": true
  },
  "info": {
    "sensor": "AHT20",
    "device": "ESP32",
    "update_interval": 30000,
    "max_records": 50,
    "timezone": "Asia/Tehran"
  }
}
